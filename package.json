{
  "name": "boilerplate-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn svg-to-react && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "svg-to-react": "svgr --icon --typescript --jsx-runtime 'automatic' -d src/components/icons public/svg",
    "generate-component": "yarn plop --plopfile generators/plop-component.js",
    "generate-state-management": "yarn plop --plopfile generators/plop-state-management.js",
    "generate-data-fetching": "yarn plop --plopfile generators/plop-data-fetching.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.5.1",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.1.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "react-redux": "^8.0.5",
    "styled-components": "^5.3.6",
    "swr": "^1.3.0"
  }
}
